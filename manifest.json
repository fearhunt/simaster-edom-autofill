{
    "manifest_version": 3,
    "name": "Simaster Auto Fill",
    "description": "Mengisi Evaluasi Dosen oleh Mahasiswa (EDOM) secara otomatis",
    "version": "1.3",
    "permissions": [
      "tabs", 
      "activeTab", 
      "storage",
      "scripting"
    ],
    "host_permissions": [ "https://simaster.ugm.ac.id/akademik/mhs_evaluasi_dosen/*" ],
    "action": {
        "default_title": "Simaster Auto Fill",
        "default_popup": "popup.html"
    }
  }